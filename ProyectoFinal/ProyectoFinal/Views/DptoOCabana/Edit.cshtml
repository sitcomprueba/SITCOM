    <div class="panel panel-white">
        <div class="panel-heading">
            <h4 class="panel-title pull-right">Editar <span class="text-bold">Habitación</span></h4>
        </div>
        <div class="panel-body">
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-horizontal">
                    <h4>DEPARTAMENTO/CABAÑA</h4>
                    <hr />
                    @Html.ValidationSummary(true)
                    @Html.HiddenFor(model => model.idCasaDptoOCabana)
                    @Html.HiddenFor(model => model.idComplejo)

                    <div class="form-group">
                        <div class="col-xs-12 col-lg-offset-1">
                            @Html.Label("Nombre: ", new { @class = "control-label" })
                           <br />
                                @Html.EditorFor(model => model.nombreCasaDptoOCabana)
                                @Html.ValidationMessageFor(model => model.nombreCasaDptoOCabana)
                            </div>
                        </div>

                    <div class="form-group">
                        <div class="col-xs-12 col-lg-offset-1">
                            @Html.Label("Cantidad de Habitaciones: ", new { @class = "control-label" })
                            <br />
                                @Html.EditorFor(model => model.cantidadHabitaciones)
                                @Html.ValidationMessageFor(model => model.cantidadHabitaciones)
                            </div>
                        </div>

                    <div class="form-group">
                        <div class="col-xs-12 col-lg-offset-1">
                            @Html.Label("Cantidad de Ambientes: ", new { @class = "control-label" })
                            <br />
                                @Html.EditorFor(model => model.cantidadAmbientes)
                                @Html.ValidationMessageFor(model => model.cantidadAmbientes)
                            </div>
                        </div>

                    <div class="form-group">
                        <div class="col-xs-12 col-lg-offset-1">
                            @Html.Label("Cantidad de baños: ", new { @class = "control-label" })
                            <br />
                                @Html.EditorFor(model => model.cantidadBanios)
                                @Html.ValidationMessageFor(model => model.cantidadBanios)
                            </div>
                        </div>

                    <div class="form-group">
                        <div class="col-xs-12 col-lg-offset-1">
                            @Html.Label("Capacidad Máxima: ", new { @class = "control-label" })
                            <br />
                                @Html.EditorFor(model => model.capacidadMaxima)
                                @Html.ValidationMessageFor(model => model.capacidadMaxima)
                            </div>
                        </div>

                    <div class="form-group">
                        <div class="col-md-offset-5 col-md-10">
                            <button type="submit" class="btn btn-green">
                                GUARDAR
                                <i class="fa fa-save"></i>
                            </button>
                        </div>
                    </div>
                </div>
            }


        </div>
    </div>
           
            <div>
